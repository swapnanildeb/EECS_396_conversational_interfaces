(in-microtheory PeopleForSocialModelingMt)

(isa userInfo TernaryPredicate)



(in-microtheory SocialModelingEnglishMt)

(genFormat userInfo "~A favorite ~A is ~A" (TheList (TheList 1 :POSSESSIVE) 2 3))



(in-microtheory EANLU-Pnames)

(indexedProperName (TheList) faraaz faraaz)
(indexedProperName (TheList) swapnanil Swapnanil)
(indexedProperName (TheList) nate Nate)
(indexedProperName (TheList) annika Annika)
(indexedProperName (TheList) jakub Jakub)



(in-microtheory (SocialModelMtFn faraaz))
(genlMt NUPeopleDataMt (SocialModelMtFn faraaz))

(isa faraaz NUPerson)
(userInfo faraaz Food Pizza)
(userInfo faraaz WheeledTransportationDevice Bicycle)

(FNNounSemtrans (TheList) Faraaz-TheWord
  (and (isa :NOUN faraaz)) (frame FN_Misc)
  (bindingTemplate (TheList)) (groupPatterns (TheSet)))



